  "devDependencies": {
    "angular": "^1.3.8",
    "angular-mocks": "^1.3.8",
    "istanbul": "^0.3.5",
    "jasmine-core": "^2.1.3",
    "karma": "^0.12.31",
    "karma-chrome-launcher": "^0.1.7",
    "karma-coverage": "^0.2.7",
    "karma-firefox-launcher": "^0.1.4",
